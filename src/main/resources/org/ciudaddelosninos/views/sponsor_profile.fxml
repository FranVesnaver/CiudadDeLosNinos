<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      stylesheets="@../css/styles.css"
      fx:controller="org.ciudaddelosninos.controller.SponsorProfileController"
      spacing="10" alignment="CENTER" style="-fx-padding: 20;">

    <Label fx:id="dniLabel"/>
    <Label fx:id="firstNameLabel"/>
    <Label fx:id="lastNameLabel"/>
    <Label fx:id="addressLabel"/>
    <Label fx:id="emailLabel"/>
    <Label fx:id="facebookLabel"/>
    <Label fx:id="landlineLabel"/>
    <Label fx:id="mobileLabel"/>
    <Label fx:id="birthDateLabel"/>
    <Label fx:id="occupationLabel"/>
    <Label fx:id="cuilCuitLabel"/>
    <Button text="Volver" onAction="#goBack"/>

    <Label fx:id="errorLabel" wrapText="true" style="error-label"/>
</VBox>
